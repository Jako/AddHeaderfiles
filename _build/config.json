{
  "name": "AddHeaderfiles",
  "lowCaseName": "addheaderfiles",
  "description": "Wrapper for regClient functions",
  "author": "Thomas Jakobi",
  "version": "1.0.0",
  "package": {
    "elements": {
      "snippets": [
        {
          "name": "AddHeaderfiles",
          "file": "addheaderfiles.snippet.php",
          "properties": [
            {
              "name": "addcode",
              "value": ""
            },
            {
              "name": "sep",
              "value": ";"
            },
            {
              "name": "sepmed",
              "value": "|"
            },
            {
              "name": "mediadefault",
              "value": "screen, tv, projection"
            }
          ]
        }
      ]
    }
  },
  "dependencies": [
    {
      "name": "php",
      "version": ">=5.4"
    },
    {
      "name": "modx",
      "version": ">=2.5"
    }
  ],
  "build": {
    "readme": "docs/readme.md",
    "license": "docs/license.md",
    "changelog": "docs/changelog.md",
    "setupOptions": {
      "source": "setup.options.php"
    },
    "resolver": {
      "after": [
        "resolve.treehillstudio.php"
      ]
    }
  }
}
